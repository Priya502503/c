#include<stdio.h>
void main()
{
    int a[10],i,n,min;
    printf("enter the size of array:");
    scanf("%d",&n);
    printf("enter the elements of array:");
    for(i=0;i<n;++i)
    {scanf("%d",&a[i]);
    }
    for(i=0;i<n;++i)
    {
    min=a[0];
    for(i=0;i<n;++i)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    }
    printf("the minimum element of array is %d",min);
}

