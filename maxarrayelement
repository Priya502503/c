#include<stdio.h>
void main()
{
    int a[10],i,n,max;
    printf("enter the size of array:");
    scanf("%d",&n);
    printf("enter the elements of array:");
    for(i=0;i<n;++i)
    {scanf("%d",&a[i]);
    }
    for(i=0;i<n;++i)
    {
    max=a[0];
    for(i=0;i<n;++i)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    }
    printf("the maximum element of array is %d",max);
}

